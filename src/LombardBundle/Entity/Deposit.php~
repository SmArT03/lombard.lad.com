<?php

namespace Lombard\LombardBundle\Entity;

use Symfony\Component\HttpKernel\Exception\NotFoundHttpException;
use Doctrine\ORM\Mapping as ORM;
use Gedmo\Mapping\Annotation as Gedmo;
use Symfony\Component\Validator\Constraints as Assert;

/**
 * @ORM\Entity
 * @Gedmo\SoftDeleteable(fieldName="deletedAt", timeAware=false)
 * @ORM\Table()
 */
class Deposit {

    use \Gedmo\Timestampable\Traits\TimestampableEntity,
        \Gedmo\Blameable\Traits\BlameableEntity,
        \Gedmo\SoftDeleteable\Traits\SoftDeleteableEntity;

    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @ORM\Column(type="date", nullable=false)
     *   
     * @Assert\Date()
     */
    private $date;
    
    /**
     * @ORM\Column(type="decimal", precision=8, scale=2, nullable=false)
     */
    private $price;

    /**
     * @ORM\Column(type="string", length=255, nullable=false)
     */
    private $deposit_term;
    
    /**
     * @ORM\Column(type="string", length=255, nullable=false)
     */
    private $status;


    /**
     * 
     * @ORM\ManyToOne(targetEntity="Client")
     * @ORM\JoinColumn(nullable=false)
     *
     */
    protected $client;
    
    /**
     * 
     * @ORM\ManyToOne(targetEntity="Product")
     * @ORM\JoinColumn(nullable=false)
     *
     */
    protected $product;



    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set date
     *
     * @param \DateTime $date
     * @return Deposit
     */
    public function setDate($date)
    {
        $this->date = $date;

        return $this;
    }

    /**
     * Get date
     *
     * @return \DateTime 
     */
    public function getDate()
    {
        return $this->date;
    }

    /**
     * Set price
     *
     * @param string $price
     * @return Deposit
     */
    public function setPrice($price)
    {
        $this->price = $price;

        return $this;
    }

    /**
     * Get price
     *
     * @return string 
     */
    public function getPrice()
    {
        return $this->price;
    }

    /**
     * Set deposit_term
     *
     * @param string $depositTerm
     * @return Deposit
     */
    public function setDepositTerm($depositTerm)
    {
        $this->deposit_term = $depositTerm;

        return $this;
    }

    /**
     * Get deposit_term
     *
     * @return string 
     */
    public function getDepositTerm()
    {
        return $this->deposit_term;
    }

    /**
     * Set status
     *
     * @param string $status
     * @return Deposit
     */
    public function setStatus($status)
    {
        $this->status = $status;

        return $this;
    }

    /**
     * Get status
     *
     * @return string 
     */
    public function getStatus()
    {
        return $this->status;
    }

    /**
     * Set client
     *
     * @param \Lombard\LombardBundle\Entity\Client $client
     * @return Deposit
     */
    public function setClient(\Lombard\LombardBundle\Entity\Client $client)
    {
        $this->client = $client;

        return $this;
    }

    /**
     * Get client
     *
     * @return \Lombard\LombardBundle\Entity\Client 
     */
    public function getClient()
    {
        return $this->client;
    }

    /**
     * Set product
     *
     * @param \Lombard\LombardBundle\Entity\Product $product
     * @return Deposit
     */
    public function setProduct(\Lombard\LombardBundle\Entity\Product $product)
    {
        $this->product = $product;

        return $this;
    }

    /**
     * Get product
     *
     * @return \Lombard\LombardBundle\Entity\Product 
     */
    public function getProduct()
    {
        return $this->product;
    }
}
